{
  "name": "euphoria",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:server": "cd server && npm run dev",
    "dev:all": "concurrently --kill-others \"npm run dev\" \"npm run dev:server\"",
    "build": "next build",
    "build:server": "cd server && npm run build",
    "build:all": "npm run build && npm run build:server",
    "start": "next start",
    "start:server": "cd server && npm run start",
    "lint": "next lint"
  },
  "dependencies": {
    "@solana/web3.js": "^1.98.4",
    "bs58": "^6.0.0",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.462.0",
    "mongodb": "^7.0.0",
    "next": "^16.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "socket.io-client": "^4.8.3",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "eslint": "^9.17.0",
    "eslint-config-next": "^16.1.1",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.2"
  }
}
